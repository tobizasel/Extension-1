{
  "name": "MeContaron Prueba",
  "description": "probando",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "./assets/logo.png"
    }
  },
  "icons": {
    "128": "./assets/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "https://www.youtube.com/*"],
      "js": ["./src/js/script.js", "./src/js/firebase.js", "./src/js/tweets.js"],
      "css": ["./src/css/tweet.css"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": ["./src/js/script.js", "./src/css/style.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
